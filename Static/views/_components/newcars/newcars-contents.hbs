        </div>
    </div>
    <div class="row double-row-gutter">
        {{#each newcars.items}}
        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-3 double-column-gutter newcar-item">
            <div class="newcar-thumbnail orange-top-border">
                <a href="{{car-url}}"><img src="{{thumbnail}}" alt="" /></a>
